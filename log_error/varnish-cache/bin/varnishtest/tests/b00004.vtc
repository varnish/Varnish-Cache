test "Torture Varnish with start/stop commands"

server s1 {
	rxreq
}

varnish v1 -vcl+backend { } 

varnish v1 -start
varnish v1 -stop

varnish v1 -start
varnish v1 -stop

varnish v1 -start
varnish v1 -stop

varnish v1 -cliok start
varnish v1 -clierr 300 start
varnish v1 -clierr 300 start

varnish v1 -cliok stop
varnish v1 -clierr 300 stop
varnish v1 -clierr 300 stop

varnish v1 -wait
