# $Id$

test "Check function calls with no action return"

server s1 {
	rxreq
	expect req.url == "/"
	expect req.http.foobar == "snafu"
	txresp -body "slash"
} -start 

varnish v1 -vcl+backend {

	sub vcl_recv {
		call some_subr;
	}

	sub some_subr {
		set req.http.foobar = "snafu";
	}

} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
} -run

